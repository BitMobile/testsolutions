		<c:Button Text="Back" OnClick="$DoBack()" OnEvent="Back" />
		<c:Button Text="Dialog.Alert" OnClick="$Alert()"/>
		<c:Button Text="Dialog.Ask" OnClick="$Ask()"/>
		<c:Button Text="Dialog.Message" OnClick="$Message()"/>
		<c:Button Text="Dialog.Debug" OnClick="$Debug()"/>
		<c:Button Text="Dialog.DateTime" OnClick="$DateTimeDialog()"/>
		<c:Button Text="Dialog.Date" OnClick="$Date()"/>
		<c:Button Text="Dialog.Time" OnClick="$Time()"/>
		<c:Button Text="Dialog.Choose" OnClick="$Choose()"/>
